{
	"name": "r3-mvp-stack",
	"dockerComposeFile": "../docker-compose.dev.yml",
	"service": "r3-mvp-stack",
	"workspaceFolder": "/workspace",
	"forwardPorts": [
		4321,
		8090
	],
	"customizations": {
